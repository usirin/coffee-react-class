{
  "name": "coffee-react-class",
  "description": "React component as a coffee-script class (autobind, mixins, ...)",
  "version": "0.1.0",
  "homepage": "https://github.com/usirin/coffee-react-class",
  "author": "Umut Sirin <umutsirin1@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git://github.com/usirin/coffee-react-class.git"
  },
  "bugs": {
    "url": "https://github.com/usirin/coffee-react-class/issues"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/usirin/coffee-react-class/blob/master/licence"
    }
  ],
  "main": "lib/index.js",
  "scripts": {
    "build": "coffee --bare --compile --output lib/ src/",
    "test": "node ./node_modules/jest-cli/bin/jest.js"
  },
  "dependencies": {
