  },
  "jest": {
    "testDirectoryName": "test",
    "scriptPreprocessor": "./node_modules/jest-utils/lib/preprocessors/coffee-react.js",
    "moduleFileExtensions": [
      "coffee",
      "js"
    ],
    "testFileExtensions": [
      "coffee",
      "js"
    ]
  },
  "devDependencies": {
    "coffee-script": "^1.9.0",
    "jest-cli": "^0.2.2",
